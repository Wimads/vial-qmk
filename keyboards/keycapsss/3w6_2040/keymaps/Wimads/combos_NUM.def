COMB(NUM_ESC,       KC_ESC,         KC_7,    KC_8,    KC_9)    //789 ESC, useful for locked numpad layer

//Navigation:
COMB(NUM_HOME,      KC_HOME,        KC_7,    KC_4)
COMB(NUM_UP,        KC_UP,          KC_8,    KC_5)
COMB(NUM_END,       KC_END,         KC_9,    KC_6)
